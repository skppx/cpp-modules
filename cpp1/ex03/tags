HumanA	HumanA.cpp	/^HumanA::HumanA(std::string name, Weapon & weapon) : _name(name), _weapon(weapon)$/;"	f	class:HumanA
HumanA	HumanA.hpp	/^class HumanA$/;"	c
HumanB	HumanB.cpp	/^HumanB::HumanB(std::string name) : _name(name)$/;"	f	class:HumanB
HumanB	HumanB.hpp	/^class HumanB$/;"	c
WEAPON_HPP	Weapon.hpp	/^#define WEAPON_HPP$/;"	d
Weapon	Weapon.cpp	/^Weapon::Weapon(std::string type)$/;"	f	class:Weapon
Weapon	Weapon.hpp	/^class Weapon$/;"	c
_name	HumanA.hpp	/^		std::string _name;$/;"	m	class:HumanA	typeref:typename:std::string
_name	HumanB.hpp	/^		std::string _name;$/;"	m	class:HumanB	typeref:typename:std::string
_type	Weapon.hpp	/^		std::string _type;$/;"	m	class:Weapon	typeref:typename:std::string
_weapon	HumanA.hpp	/^		Weapon&		_weapon;$/;"	m	class:HumanA	typeref:typename:Weapon &
_weapon	HumanB.hpp	/^		Weapon*		_weapon;$/;"	m	class:HumanB	typeref:typename:Weapon *
attack	HumanA.cpp	/^void	HumanA::attack(void)$/;"	f	class:HumanA	typeref:typename:void
attack	HumanB.cpp	/^void	HumanB::attack(void) const$/;"	f	class:HumanB	typeref:typename:void
getType	Weapon.cpp	/^std::string const & Weapon::getType(void)$/;"	f	class:Weapon	typeref:typename:std::string const &
main	main.cpp	/^int main()$/;"	f	typeref:typename:int
setType	Weapon.cpp	/^void	Weapon::setType(std::string type)$/;"	f	class:Weapon	typeref:typename:void
setWeapon	HumanB.cpp	/^void	HumanB::setWeapon(Weapon & weapon)$/;"	f	class:HumanB	typeref:typename:void
~HumanA	HumanA.cpp	/^HumanA::~HumanA()$/;"	f	class:HumanA
~HumanB	HumanB.cpp	/^HumanB::~HumanB()$/;"	f	class:HumanB
~Weapon	Weapon.cpp	/^Weapon::~Weapon()$/;"	f	class:Weapon
