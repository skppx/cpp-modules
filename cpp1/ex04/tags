SEV_HPP	Sed.hpp	/^#define SEV_HPP$/;"	d
Sed	Sed.cpp	/^Sed::Sed(std::string filename) : _infile(filename)$/;"	f	class:Sed
Sed	Sed.hpp	/^class Sed$/;"	c
_infile	Sed.hpp	/^		std::string _infile;$/;"	m	class:Sed	typeref:typename:std::string
_outfile	Sed.hpp	/^		std::string _outfile;$/;"	m	class:Sed	typeref:typename:std::string
main	main.cpp	/^int main(int argc, char **argv)$/;"	f	typeref:typename:int
replace	Sed.cpp	/^void	Sed::replace(std::string s1, std::string s2)$/;"	f	class:Sed	typeref:typename:void
~Sed	Sed.cpp	/^Sed::~Sed(void)$/;"	f	class:Sed
